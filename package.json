{
  "name": "vue3-base",
  "version": "0.0.1",
  "description": "vue3+ts基础管理平台",
  "keywords": [
    "vue3",
    "ts",
    "typescript",
    "tdesign",
    "tdesign-vue-next"
  ],
  "license": "MIT",
  "author": "948533798@qq.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/wc19950724/vue3-tdesign.git"
  },
  "bugs": {
    "url": "https://github.com/wc19950724/vue3-tdesign/issues"
  },
  "main": "src/main.ts",
  "scripts": {
    "start": "vite",
    "build": "yarn lint && vite build",
    "lint": "yarn vue-tsc && yarn prettier && yarn eslint",
    "vue-tsc": "vue-tsc -p tsconfig.json",
    "prettier": "prettier --ignore-path .gitignore . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --write",
    "eslint": "eslint --ignore-path .gitignore . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@vueuse/core": "^9.6.0",
    "dayjs": "^1.11.7",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^3.0.1",
    "tdesign-vue-next": "^1.0.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.11.11",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "ghooks": "^2.0.4",
    "less": "^4.1.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.2.2",
    "vite-plugin-html": "^3.2.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.0.11"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "yarn lint"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npx lint-staged --quiet --concurrent false"
    }
  }
}
